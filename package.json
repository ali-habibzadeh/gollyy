{
  "name": "gollyy",
  "version": "0.0.1",
  "description": "Golly Lottery",
  "main": "index.js",
  "repository": "https://github.com/ali-habibzadeh/gollyy.git",
  "author": "Ali Habibzadeh <ali.hci@icloud.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "build": "tsc",
    "copyall": "cp package.json dist/package.json",
    "postbuild": "yarn copyall && cd dist && npm install --only=production && node-prune",
    "watch": "tsc -w",
    "test": "jest --config=jest.config.js",
    "deploy": "yarn build && cdk deploy GollyyLottery --require-approval never --outputs-file outputs.json",
    "deploy-static": "yarn build && cdk deploy GollyyLotteryStatic",
    "destroy": "cdk destroy GollyyLottery -f",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@aws-cdk/aws-appsync": "^1.77.0",
    "@aws-cdk/aws-certificatemanager": "^1.77.0",
    "@aws-cdk/aws-cognito": "^1.77.0",
    "@aws-cdk/aws-dynamodb": "^1.77.0",
    "@aws-cdk/aws-ec2": "^1.77.0",
    "@aws-cdk/aws-lambda": "^1.77.0",
    "@aws-cdk/aws-route53": "^1.77.0",
    "@aws-cdk/aws-route53-targets": "^1.77.0",
    "@aws-cdk/core": "^1.77.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/convict": "^5.2.1",
    "@types/jest": "^26.0.17",
    "@types/source-map-support": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "aws-cdk": "^1.77.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "node-prune": "^1.0.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^4.5.5",
    "aws-sdk": "^2.805.0",
    "convict": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
