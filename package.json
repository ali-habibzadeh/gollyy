{
  "name": "gollyy",
  "version": "0.0.1",
  "description": "Golly Lottery",
  "main": "index.js",
  "repository": "https://github.com/ali-habibzadeh/gollyy.git",
  "author": "Ali Habibzadeh <ali.hci@icloud.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "build": "tsc",
    "copyall": "cp package.json dist/package.json",
    "postbuild": "yarn copyall && cd dist && npm install --only=production && node-prune",
    "watch": "tsc -w",
    "integration": "jest --config=jest.integration.config.js",
    "deploy": "yarn build && cdk deploy --require-approval never --outputs-file outputs.json",
    "destroy": "cdk destroy -f",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "node-prune": "^1.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  }
}
